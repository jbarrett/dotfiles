alias mc="mc -d"
alias yt="quvi --exec 'mplayer %u'"
alias youtube="quvi --exec 'mplayer %u'"
alias vi=vim
alias dosbox="dosbox -conf dosbox.conf ."
alias gti='git'
alias igt='git'
alias got='git'

if [ -f /etc/slackware-version ] ; then
    alias sbopkg="sudo sbopkg"
    alias pkgtool="sudo pkgtool"
    alias slackpkg="sudo slackpkg"
    alias upgradepkg="sudo upgradepkg --reinstall --install-new"
    alias installpkg="sudo installpkg"
fi
